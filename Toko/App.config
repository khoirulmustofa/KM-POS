<?xml version="1.0" encoding="utf-8"?>
<configuration>
    <startup> 
        <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.0"/>
    </startup>
<connectionStrings>
  <add name="TokoConstring" connectionString="Server=127.0.0.1;Port=3306;Database=toko;Uid=root;Pwd=;Allow User Variables=True" providerName="MySql.Data.MySqlClient" />
</connectionStrings>
  <system.data>
    <DbProviderFactories>
      <add name="MySQL Data Provider" invariant="MySql.Data.MySqlClient" description=".Net Framework Data Provider for MySQL" type="MySql.Data.MySqlClient.MySqlClientFactory,MySql.Data" />
    </DbProviderFactories>
  </system.data>
</configuration>
